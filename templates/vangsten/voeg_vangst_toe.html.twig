{% extends 'base.html.twig' %}
{% block title %}Carpp Vangst Toevoegen{% endblock %}
{% block body %}
    <h1 class="text-center mainGreenColor">Voeg vangst toe</h1>
    <div class="grid-x grid-padding-x grid-padding-y">
        <div class="cell">
            <a class="button float-left mainGreenBackground " href="{{ path('app_vangsten') }}">Terug</a>
        </div>
    </div>
    <div class ="grid-x align-center">
            <div class="cell large-6 medium-8 small-10">
                {{ form(vangsten_form) }}
            </div>
    </div>
    {% block javascripts %}
        {{ parent() }}
        {{ encore_entry_script_tags('app_weather') }}
        {% endblock %}
{% endblock %}